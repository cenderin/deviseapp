<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>
<% @users.each do |user| %>
	<div class="media">
		<a class="pull-left" href="#">
			<%= image_tag(gravatar_url(user.email), class: "media-object") %>
		</a>
		<div class="media-body">
			<h4 class="media-heading"><%= user.name %></h4>
			<div class="media">
			</div>
		</div>
	</div>


<% end %>